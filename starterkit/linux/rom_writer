#! /bin/bash
#===============================
#
# rom_writer
#
# 2021/10/20 Kuninori Morimoto <kuninori.morimoto.gx@renesas.com>
#===============================
TOP=`readlink -f "$0" | xargs dirname`

if [ $# != 5  ]; then
	${TOP}/script/help ${TOP} rom_writer
	exit
fi

${TOP}/script/rom_writer $@
