;====================================
;
; Renesas StarterKit ROM Writer
;
;====================================
;=============================
; set 1 if you want to be asked
;=============================
ask_in_main_loop = 0

;=============================
; top dir
;=============================
getdir top
dirname top top

;=============================
;
; SoC type and Files
;
;=============================
num_soc = 6
strdim soc	num_soc
strdim soc_file	num_soc

soc[0] = "H3    (S/N     0 - 2000)" ; WS1.1 is not supported
soc[1] = "H3 4G (S/N  2001 - 4500)"
soc[2] = "H3 8G (S/N 20001 - )"
soc[3] = "M3 2G (S/N     0 - 2000)"
soc[4] = "M3 8G (S/N  2001 - )"
soc[5] = "M3N   (S/N all)"

soc_file[0] = ""
soc_file[1] = "h3_4g"
soc_file[2] = "h3_8g"
soc_file[3] = "m3_2g"
soc_file[4] = "m3_8g"
soc_file[5] = "m3n"

;=============================
;
; Auto Mode
;
;=============================
auto_mode = "starterkit"

;=============================
;
; Max list number
; Max DipSwitch setting message number
;
; see below settings on each soc settings file
;
;	list
;	dipswitch_h_update
;	dipswitch_h_normal
;
;=============================
num_list = 6
num_dipswitch_a = 0
num_dipswitch_h = 4

;=============================
;
; run main loop
;
;=============================
sprintf2 lib "%s\windows\rom_writer.lib" top
include lib
end
